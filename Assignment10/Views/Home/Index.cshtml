@{
    ViewData["Title"] = "Home Page";
}

@model IndexViewModel 

<h1>Bowlers Contact List</h1>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Middle Initial</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.Bowlers)
        {
            <tr>
                <td>@x.BowlerFirstName</td>
                <td>@x.BowlerMiddleInit</td>
                <td>@x.BowlerLastName</td>
                <td>@x.BowlerAddress</td>
                <td>@x.BowlerCity</td>
                <td>@x.BowlerState</td>
                <td>@x.BowlerZip</td>
                <td>@x.BowlerPhoneNumber</td>
            </tr>
        }
    </tbody>
</table>

<div page-info="@Model.PageNumberingInfo" page-url-teamcategory="@Model.TeamCategory"></div>